{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": "arn:aws:s3:::splatntreat/scenes/*"
        },
        {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::splatntreat",
            "Condition": {
                "StringLike": {
                    "s3:prefix": "scenes/*"
                }
            }
        }
    ]
}
